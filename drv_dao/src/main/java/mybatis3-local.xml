<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
<properties resource="kia_oracle.property"></properties>

<typeAliases>
   <typeAlias type="com.trusteach.drv.model.BookedWeekRecordModel" alias="BookedWeekRecordModel"/>
   <typeAlias type="com.trusteach.drv.model.BookedLimitModel" alias="BookedLimitModel"/>
 <typeAlias type="com.trusteach.drv.model.DictModel" alias="DictModel"/>
</typeAliases>

<plugins>
	<plugin interceptor="com.smartken.kia.core.plugin.mybatis.OffsetLimitInterceptor">
		<property name="dialectClass" value="com.smartken.kia.core.jdbc.dialect.OracleDialect"/>
	</plugin>
</plugins>

<environments default="development">
<environment id="development">
<transactionManager type="JDBC"/>
<dataSource type="POOLED">
<property name="driver" value="${driver}"/>
<property name="url" value="${url}"/>
<property name="username" value="${username}"/>
<property name="password" value="${password}"/>
</dataSource>
</environment>
</environments>

<mappers>
<mapper resource="com/trusteach/drv/mapper/booked/IWeekRecordMapper.xml"/>
<mapper resource="com/trusteach/drv/mapper/booked/ILimitMapper.xml"/>
<mapper resource="com/trusteach/drv/mapper/sys/IDictMapper.xml"/>
</mappers>

</configuration>