<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>

<package name="sys-menu"  extends="base-package" namespace="/sys/menu" >

   <action name="fn-*" 
     class="com.smartken.kia.web.struts2.sys.MenuAction" method="{1}">
     <result name="success">/sys/menu/index.jsp</result>
     <result name="input">/sys/menu/{1}.jsp</result>
   </action>
   <action name="*-*" 
     class="com.smartken.kia.web.struts2.sys.MenuAction" method="{1}_{2}">
      <result name="input">/sys/menu/{2}.jsp</result>
   </action>

 
</package>




</struts>
