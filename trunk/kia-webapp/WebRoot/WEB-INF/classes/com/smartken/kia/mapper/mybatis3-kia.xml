<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

<typeAliases>
   <typeAlias type="com.smartken.kia.model.sys.MenuModel" alias="MenuModel"/>
   <typeAlias type="com.smartken.kia.model.sys.UserModel" alias="UserModel"/>
   <typeAlias type="com.smartken.kia.model.sys.DepartmentModel" alias="DepartmentModel"/>
   <typeAlias type="com.trusteach.drv.model.BookedWeekRecordModel" alias="BookedWeekRecordModel"/>
</typeAliases>


<plugins>
	<plugin interceptor="com.smartken.kia.core.plugin.mybatis.OffsetLimitInterceptor">
		<property name="dialectClass" value="com.smartken.kia.core.jdbc.dialect.OracleDialect"/>
	</plugin>
</plugins>


